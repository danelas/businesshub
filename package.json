{
  "name": "llc-lead-generator",
  "version": "1.0.0",
  "description": "Automated LLC lead generation and outreach system",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "worker": "node src/workers/messageWorker.js",
    "ingest": "node src/scripts/ingestData.js",
    "load-templates": "node src/scripts/loadTemplates.js",
    "florida-sync": "node src/scripts/floridaSync.js",
    "setup-florida": "node src/database/migrate.js && node -e \"require('fs').readFileSync('src/database/florida_schema.sql', 'utf8').split(';').filter(s => s.trim()).forEach(sql => require('./src/database/connection').query(sql).catch(console.error))\"",
    "test-florida": "node test-florida-pipeline.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "axios": "^1.5.0",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "@sendgrid/mail": "^7.7.0",
    "textmagic-rest-nodejs": "^2.0.1",
    "joi": "^17.9.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.10.0",
    "ssh2-sftp-client": "^9.1.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "lead-generation",
    "llc",
    "automation",
    "sms",
    "email",
    "outreach"
  ],
  "author": "",
  "license": "MIT"
}
